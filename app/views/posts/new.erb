<h1>NEW LOBANG</h1>

<div class="form-wrapper">

  <%= simple_form_for @post, html: { class: 'form-horizontal', multipart: true } do |f| %>
    <div class="form01">
      <%= f.input :title %>
    </div>

    <div class="form01">
      <%= f.input :image %>
    </div>

    <div class="form01">
      <%= f.input :country, as: :country %>
    </div>

    <div class="form01">
      <%= f.input :budget %>
    </div>

    <div class="form01">
      <%= f.input :description %>
    </div>

    <div class="form01">
      <%= f.button :submit, class: 'btn-success' %>
    </div>

  <% end %>

</div>
